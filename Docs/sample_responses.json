{
  "source": "Contracts Finder API - Public CSV Export",
  "endpoint": "https://www.contractsfinder.service.gov.uk/Search/GetCsvFile",
  "query_params": {
    "keyword": "NHS Trust",
    "published": 90
  },
  "date_captured": "2025-10-01",
  "total_results": "~849 notices",

  "csv_structure": {
    "format": "CSV with headers",
    "delimiter": ",",
    "quote_char": "\"",
    "total_fields": 42,
    "encoding": "UTF-8"
