  "field_definitions": {
    "Notice Identifier": {
      "type": "string (UUID)",
      "required": true,
      "example": "23f2f473-8e8d-4a38-b8ba-8535ee7cb1de",
      "description": "Unique identifier for the tender notice"
    },
    "Notice Type": {
      "type": "string",
      "required": true,
      "example": "Contract",
      "possible_values": ["Contract", "Award", "Future Opportunity"],
      "description": "Type of procurement notice"
    },
    "Organisation Name": {
      "type": "string",
      "required": true,
      "example": "St George's University Hospitals NHS Foundation Trust",
      "description": "Name of the buying organization"
    },
    "Status": {
      "type": "string",
      "required": true,
      "example": "Open",
      "possible_values": ["Open", "Closed", "Awarded", "Cancelled"],
      "description": "Current status of the tender"
    },
    "Published Date": {
      "type": "date",
      "required": true,
      "format": "DD MMMM YYYY",
      "example": "30 September 2025",
      "description": "Date the notice was published"
    },
    "Title": {
      "type": "string",
      "required": true,
      "example": "LTHW Constant&Variable Temperature Plant/Eqpt Refurb Works",
      "description": "Tender title"
    },
    "Description": {
      "type": "text",
      "required": false,
      "example": "Tender for LTHW Constant & Variable Temperature Centre Plant/Equipment Refurbishment Works",
      "description": "Detailed description of the tender"
    },
    "Value Low": {
      "type": "number",
      "required": false,
      "example": "1000000",
      "description": "Minimum contract value in GBP"
    },
    "Value High": {
      "type": "number",
      "required": false,
      "example": "1000000",
      "description": "Maximum contract value in GBP"
    },
    "Closing Date": {
      "type": "date",
      "required": false,
      "format": "DD MMMM YYYY",
      "example": "15 October 2025",
      "description": "Deadline for tender submissions"
    },
    "Closing Time": {
      "type": "time",
      "required": false,
      "format": "HH:MM",
      "example": "12:00",
      "description": "Specific time for tender deadline"
    },
    "Postcode": {
      "type": "string",
      "required": false,
      "example": "SW17 0QT",
      "description": "Location postcode"
    },
    "Region": {
      "type": "string",
      "required": false,
      "example": "London",
      "possible_values": ["London", "South East", "North West", "Wales", "Scotland", "etc."],
      "description": "Geographic region"
    },
    "Cpv Codes": {
      "type": "string (comma-separated)",
      "required": false,
      "example": "45331100,45331200",
      "description": "Common Procurement Vocabulary classification codes"
    },
    "OJEU Contract Type": {
      "type": "string",
      "required": false,
      "example": "Works",
      "possible_values": ["Goods", "Services", "Works"],
      "description": "Type of contract"
    },
    "Contact Name": {
      "type": "string",
      "required": false,
      "example": "John Smith",
      "description": "Buyer contact person"
    },
    "Contact Email": {
      "type": "string",
      "required": false,
      "example": "procurement@trust.nhs.uk",
      "description": "Buyer contact email"
    },
    "Contact Telephone": {
      "type": "string",
      "required": false,
      "example": "020 1234 5678",
      "description": "Buyer contact phone"
    },
    "Suitable for SME": {
      "type": "string",
      "required": false,
      "example": "Yes",
      "possible_values": ["Yes", "No", ""],
      "description": "Whether suitable for small/medium enterprises"
    },
    "Suitable for VCO": {
      "type": "string",
      "required": false,
      "example": "Yes",
      "possible_values": ["Yes", "No", ""],
      "description": "Whether suitable for voluntary/community organizations"
    }
  },

